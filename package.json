{
  "scripts": {
    "start:server": "rm -rf ./build && webpack --config  webpack.config.server.js",
    "start:client": "webpack-dev-server --config webpack.config.client.js",
    "start": "rm -rf ./.build && NODE_ENV=development npm-run-all --parallel  start:server start:client",
    "start:prod": "NODE_ENV=production node ./build/server.js"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.23.0",
    "npm-run-all": "^4.0.2",
    "react-dev-utils": "^0.5.2",
    "react-hot-loader": "3.0.0-beta.6",
    "start-server-webpack-plugin": "^2.1.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "source-map-support": "^0.4.11"
  }
